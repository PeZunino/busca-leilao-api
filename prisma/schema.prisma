generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Auctioneer{
  id String @id @default(uuid())
	number String
	cep String
	neighborhood String
	city String
	name String
	registrationCode String
	phone String
	email String @unique
	website String
  street String
  createdAt    DateTime  @default(now()) @map("created_at")
  updatedAt    DateTime? @updatedAt @map("updated_at")
}